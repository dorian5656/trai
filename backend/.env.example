# =============================================================================
# YOLO 分类配置 (itzx) - RRDSPPG
# =============================================================================
# 必需的分类列表 (逗号分隔)
RRDSPPG_YOLO_REQUIRED_CLASSES=红心,已点赞
# 任务类型 ID - 公众号转发 (OCR)
RRDSPPG_TASK_TYPE_OFFICIAL_ACCOUNT=1997929948761825282
# 任务类型 ID - 视频号 (YOLO)
RRDSPPG_TASK_TYPE_VIDEO_ACCOUNT=1996827967950909442

# =============================================================================
# 数据库配置 (PostgreSQL)
# =============================================================================
# 数据库服务器地址
POSTGRES_SERVER=your_postgres_server
# 数据库端口号
POSTGRES_PORT=your_postgres_port
# 数据库用户名
POSTGRES_USER=your_postgres_user
# 数据库密码
POSTGRES_PASSWORD=your_postgres_password
# 数据库名称
POSTGRES_DB=your_postgres_db
POSTGRES_DB_YIBAO=your_postgres_db_yibao

# =============================================================================
# 企业微信配置
# =============================================================================
# 企业微信机器人 Webhook Key（群通知用）
WECOM_TRAI_ROBOT_KEY=your_wecom_robot_key
# 企业ID
WECOM_CORP_ID=your_corp_id
# 应用Secret
WECOM_CORP_SECRET=your_corp_secret

# =============================================================================
# 飞书配置
# =============================================================================
# 飞书机器人 Webhook Token (TRAI)
FEISHU_TRAI_WEBHOOK_TOKEN=your_feishu_webhook_token
# 飞书应用凭据
FEISHU_APP_ID=your_feishu_app_id
FEISHU_APP_SECRET=your_feishu_app_secret

# 纷享销客直连推送配置
# 是否模拟运行（1=仅通知不推送，0=真实推送）
FXIAOKE_DRY_RUN=0
# 直连推送地址（外部>CRM 医保码档案推送）
FXIAOKE_DIRECT_POST_URL=https://hwcloud.fxiaoke.com/erp/syncdata/open/objdata/push
# 额外 Header（JSON 字符串）
FXIAOKE_DIRECT_POST_HEADERS={}
# 数据中心 ID（固定值）
FXIAOKE_DC_ID=your_fxiaoke_dc_id
# 租户 ID（固定值）
FXIAOKE_TENANT_ID=your_fxiaoke_tenant_id
# 接口 Token（固定值）
FXIAOKE_TOKEN=your_fxiaoke_token
# 进度推送步长（每处理多少条发送一次进度）
FXIAOKE_PROGRESS_STEP=100

# =============================================================================
# 安全配置 (JWT)
# =============================================================================
# JWT 密钥 (生成方式: openssl rand -hex 32)
SECRET_KEY=your_secret_key_change_me
# JWT 算法
ALGORITHM=HS256
# Token 过期时间 (分钟)
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# =============================================================================
# DeepSeek API 配置
# =============================================================================
DEEPSEEK_API_BASE=https://api.deepseek.com
AI_API_KEY=your_deepseek_api_key

# =============================================================================
# S3 对象存储配置 (可选)
# =============================================================================
# 是否启用 S3 上传 (true/false)
S3_ENABLED=false
# S3 服务地址 (如 MinIO: http://127.0.0.1:9000, AWS: https://s3.amazonaws.com)
S3_ENDPOINT_URL=http://127.0.0.1:9000
# Access Key
S3_ACCESS_KEY=your_access_key
# Secret Key
S3_SECRET_KEY=your_secret_key
# 存储桶名称 (类似于文件夹，用于存放文件，需唯一)
S3_BUCKET_NAME=trai-uploads
# 区域名称 (服务器所在地区，如 us-east-1, cn-north-1，MinIO 通常默认为 us-east-1)
S3_REGION_NAME=us-east-1
# 公网访问域名 (可选，用于生成 CDN URL)
S3_PUBLIC_DOMAIN=http://127.0.0.1:9000/trai-uploads

# =============================================================================
# Dify AI 配置
# =============================================================================
# Dify API 基础地址
DIFY_API_BASE_URL=http://your_dify_api_base_url/v1
# Dify API 密钥 (官网助手)
DIFY_GUANWANG_API_KEY=your_guanwang_api_key
# Dify API 密钥 (财务助手 - 预留)
DIFY_CAIWU_API_KEY=your_caiwu_api_key

# =============================================================================
# PostgreSQL 数据库配置
# =============================================================================
POSTGRES_SERVER=127.0.0.1
POSTGRES_PORT=35433
POSTGRES_USER=itzx
POSTGRES_PASSWORD=T@2026...
POSTGRES_DB=itzx
